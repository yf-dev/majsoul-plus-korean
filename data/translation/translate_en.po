msgstr "Ron!"

#: translate_sheet.csv:2765
msgctxt "Tsumo!"
msgid "sheet|VoiceSound.csv|words_en|2765"
msgstr "Tsumo!"

#: translate_sheet.csv:2766
msgctxt "Yay! Kana-chan here. I’m the TOP1 idol!"
msgid "sheet|VoiceSound.csv|words_en|2766"
msgstr "Yay! Kana-chan here. I’m the TOP1 idol!"

#: translate_sheet.csv:2767
msgctxt "Thank you for choosing me. I will keep working hard in future!"
msgid "sheet|VoiceSound.csv|words_en|2767"
msgstr "Thank you for choosing me. I will keep working hard in future!"

