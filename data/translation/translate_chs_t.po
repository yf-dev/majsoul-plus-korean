#: xinshouyindao_chs_t.json
msgctxt "Try another one-nya!"
msgid ""
"json-"
"xinshouyindao|xinshouyindao_en.json|course9|text|1f0de0fee2f33455bfe26d9d628c4eee"
msgstr "再來一題喵~"

#: xinshouyindao_chs_t.json
msgctxt "Ichihime"
msgid ""
"json-"
"xinshouyindao|xinshouyindao_en.json|course9_wrong1|name|f82699b7e09682c216a9f9fefb44642b"
